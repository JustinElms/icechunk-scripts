{
    "name":"giops_day",
    "data_path_root": "/data/hpfx.collab.science.gc.ca/",
    "data_path_template": "/data/hpfx.collab.science.gc.ca/{forecast_date}/WXO-DD/model_giops/netcdf/lat_lon/3d/{forecast_run}/{forecast_time}/*{variable}*.nc",
    "dims": ["time", "depth", "latitude", "longitude"],
