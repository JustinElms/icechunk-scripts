    "template_keys":["forecast_date","forecast_run","forecast_time","variable"],
    "path_filters": [
        "_DBS-all_"
    ],
    "variables": [
        "sokaraml",
        "vozocrtx",
        "vomecrty",
        "sossheig",
        "votemper",
        "vosaline",
        "somixhgt"
    ],
    "variable_map": {
        "sokaraml": "MixedLayerThickness",
        "vozocrtx": "SeaWaterVelocityX",
        "vomecrty": "SeaWaterVelocityY",
        "sossheig": "SeaSfcHeight",
        "votemper": "SeaWaterPotentialTemp",
        "vosaline": "SeaWaterSalinity",
        "somixhgt": "TurboclineDepth"
