{
    "name": "ciops-salish-sea_fc_3dll",
    "data_path_template": "/data/hpfx.collab.science.gc.ca/{forecast_date}/WXO-DD/model_ciops/salish-sea/500m/{forecast_run}/{forecast_time}/*_MSC_CIOPS-SalishSea_{variable}_DBS-all_*.nc",
    "template_keys": [
        "forecast_date",
        "forecast_run",
        "forecast_time",
        "variable"
    ],
    "variable_map": {
        "SeaWaterSalinity": "vomecrty",
        "SeaWaterPotentialTemp": "votemper",
        "SeaWaterVelocityY": "vosaline",
        "SeaWaterVelocityX": "vozocrtx"
    },
    "drop_vars": [
        "latitude_longitude"
    ],
    "time_dim_units": "seconds since 1950-01-01 00:00:00"
}