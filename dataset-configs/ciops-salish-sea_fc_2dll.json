{
    "name": "ciops-salish-sea_fc_2dll",
    "data_path_template": "/data/hpfx.collab.science.gc.ca/{forecast_date}/WXO-DD/model_ciops/salish-sea/500m/{forecast_run}/{forecast_time}/*_MSC_CIOPS-SalishSea_{variable}_*.nc",
    "template_keys": [
        "forecast_date",
        "forecast_run",
        "forecast_time",
        "variable"
    ],
    "path_filters": [
        "_DBS-all_"
    ],
    "variable_map": {
        "MixedLayerThickness": "sokaraml",
        "SeaWaterVelocityX": "vozocrtx",
        "SeaWaterVelocityY": "vomecrty",
        "SeaSfcHeight": "sossheig",
        "SeaWaterPotentialTemp": "votemper",
        "SeaWaterSalinity": "vosaline",
        "TurboclineDepth": "somixhgt"
    },
    "drop_vars": [
        "latitude_longitude"
    ],
    "time_dim_units": "seconds since 1950-01-01 00:00:00"
}